                    <xsd:element minOccurs="0" maxOccurs="1" name="UKResidentFor7OutOf9YearsUKResident" type="MTR:MTR_YesType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="LessThan18Years" type="MTR:MTR_YesType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="NominatedIncomeAmount" type="MTR:MTR_SAnonNegativeNonZeroMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="NominatedCapitalGainsAmount" type="MTR:MTR_SAnonNegativeNonZeroMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="AdjustmentToPaymentsOnAccount" type="MTR:MTR_SAnonNegativeMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="RemittedIncomeOrGains" type="MTR:MTR_YesType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="InvestmentInQualifyingBusiness">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element minOccurs="1" maxOccurs="1" name="AmountOfReliefClaimedForInvestmentInQualifyingBusiness" type="MTR:MTR_SAnonNegativeNonZeroMonetaryType"/>
                          <xsd:element minOccurs="1" maxOccurs="3" name="CompanyRegistrationNumber">
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="8"/>
                                <xsd:pattern value="[a-zA-Z0-9]+"/>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" maxOccurs="1" name="PreviousInvestmentNoLongerQualifies" type="MTR:MTR_YesType"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="0" maxOccurs="1" name="AnyOtherInformationSpace">
                <xsd:simpleType>
                  <xsd:restriction base="MTR:MTR_SAnonEmptyStringType">
                    <xsd:minLength value="1"/>
                    <xsd:maxLength value="20480"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="1" maxOccurs="1" name="SA110">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element minOccurs="1" maxOccurs="1" name="SelfAssessment">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="1" maxOccurs="1" name="TotalTaxEtcDue" type="MTR:MTR_SAmonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="StudentLoanRepaymentDue" type="MTR:MTR_SAnonNegativeMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="Class4NICsDue" type="MTR:MTR_SAnonNegativeMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="Class2NICsDue" type="MTR:MTR_SAnonNegativeMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="CapitalGainsTaxDue" type="MTR:MTR_SAnonNegativeNonZeroMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="PensionChargesDue" type="MTR:MTR_SAnonNegativeNonZeroMonetaryType"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="1" maxOccurs="1" name="UnderpaidTax">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="1" name="UnderpaidTaxForEarlierYearsIncludedInCode" type="MTR:MTR_SAnonNegativeMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="UnderpaidTaxForYearIncludedInFutureCode" type="MTR:MTR_SAnonNegativeMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="OutstandingDebtCodedOutAmount" type="MTR:MTR_SAnonNegativeMonetaryType"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="0" maxOccurs="1" name="PaymentsOnAccount">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="1" name="ClaimToReducePaymentsOnAccount" type="MTR:MTR_YesType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="FirstPaymentOnAccount" type="MTR:MTR_SAnonNegativeMonetaryType"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="0" maxOccurs="1" name="SurplusAllowances">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="1" name="SurplusBlindPersonsAllowance" type="MTR:MTR_SAnonNegativeNonZeroMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="SurplusMarriedCouplesAllowance" type="MTR:MTR_SAnonNegativeNonZeroMonetaryType"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="0" maxOccurs="1" name="AdjustmentsToTaxDue">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="1" name="IncreaseInTaxFromAdjustmentToEarlierYears" type="MTR:MTR_SAnonNegativeNonZeroMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="DecreaseInTaxFromAdjustmentToEarlierYears" type="MTR:MTR_SAnonNegativeNonZeroMonetaryType"/>
                    <xsd:element minOccurs="0" maxOccurs="1" name="NextYearsRepaymentClaimedNow" type="MTR:MTR_SAnonNegativeNonZeroMonetaryType"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="0" maxOccurs="1" name="AnyOtherInformationSpace">
                <xsd:simpleType>
                  <xsd:restriction base="MTR:MTR_SAnonEmptyStringType">
                    <xsd:minLength value="1"/>
                    <xsd:maxLength value="20480"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="0" maxOccurs="1" name="WelshReturn" type="MTR:MTR_YesType"/>
        <xsd:element minOccurs="0" maxOccurs="1" name="TaxpayerName">
          <xsd:simpleType>
            <xsd:restriction base="MTR:MTR_SAstringType">
              <xsd:maxLength value="56"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element minOccurs="1" maxOccurs="1" name="Declaration">
          <xsd:complexType>
            <xsd:choice minOccurs="1" maxOccurs="1">
              <xsd:element minOccurs="1" maxOccurs="1" name="IndividualDeclaration" type="MTR:MTR_YesType"/>
              <xsd:element minOccurs="1" maxOccurs="1" name="AgentDeclaration" type="MTR:MTR_YesType"/>
            </xsd:choice>
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="0" name="AttachedFiles">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element maxOccurs="unbounded" name="Attachment">
                <xsd:complexType>
                  <xsd:simpleContent>
                    <xsd:extension base="xsd:base64Binary">
                      <xsd:attribute use="required" name="FileFormat">
                        <xsd:simpleType>
                          <xsd:restriction base="MTR:MTR_SAstringType">
                            <xsd:enumeration value="pdf"/>
                          </xsd:restriction>
                        </xsd:simpleType>
                      </xsd:attribute>
                      <xsd:attribute use="required" name="Filename">
                        <xsd:simpleType>
                          <xsd:restriction base="MTR:MTR_SAfilenameType">
                            <xsd:pattern value=".*\.pdf"/>
                          </xsd:restriction>
                        </xsd:simpleType>
                      </xsd:attribute>
                      <xsd:attribute use="optional" name="Size" type="xsd:integer"/>
                      <xsd:attribute use="optional" name="Description">
                        <xsd:simpleType>
                          <xsd:restriction base="MTR:MTR_SAstringType">
                            <xsd:pattern value="[^£$#~|€]+"/>
                          </xsd:restriction>
                        </xsd:simpleType>
                      </xsd:attribute>
                    </xsd:extension>
                  </xsd:simpleContent>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute use="optional" name="AmendedReturn" type="MTR:MTR_YesType"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="MTR_ContactDetailsStructure">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="1" name="Name">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="Ttl">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:minLength value="1"/>
                  <xsd:maxLength value="4"/>
                  <xsd:pattern value="[A-Za-z][A-Za-z&apos;\-]*"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="2" name="Fore">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:minLength value="1"/>
                  <xsd:maxLength value="35"/>
                  <xsd:pattern value="[A-Za-z][A-Za-z&apos;\-]*"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="Sur">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:minLength value="1"/>
                  <xsd:maxLength value="35"/>
                  <xsd:pattern value="[A-Za-z0-9 ,\.\(\)/&amp;\-&apos;]+"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Email">
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="MTR:MTR_EmailType">
              <xsd:attribute use="optional" name="Type" type="MTR:MTR_WorkHomeType"/>
              <xsd:attribute use="optional" name="Preferred" type="MTR:MTR_YesNoType"/>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Telephone" type="MTR:MTR_TelephoneStructure"/>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Fax" type="MTR:MTR_TelephoneStructure"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="MTR_EmailType">
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="3"/>
      <xsd:maxLength value="254"/>
      <xsd:pattern value="[^@&apos;&lt;&gt;&quot;]+@[^@&apos;&lt;&gt;&quot;]+"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_IRdecimalType">
    <xsd:restriction base="xsd:decimal">
      <xsd:pattern value="-?(([1-9][0-9]*)|0)\.[0-9]{2}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_IRheaderStringType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Za-z0-9 &amp;&apos;\(\)\*,\-\./]*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_ISOcurrencyType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="EUR">
        <xsd:annotation>
          <xsd:documentation>Euro</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="GBP">
        <xsd:annotation>
          <xsd:documentation>Sterling</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="MTR_SA106SourceOfForeignIncomeTotals">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="1" name="SWTOrUKTax" type="MTR:MTR_SAnonNegativeNonZeroMonetaryType"/>
      <xsd:element minOccurs="0" maxOccurs="1" name="TaxableAmount" type="MTR:MTR_SAnonNegativeMonetaryType"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="MTR_SA28charStringType">
    <xsd:restriction base="MTR:MTR_SAstringType">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="28"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SACGTreliefCodeType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z]{3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAMTRNINOtype">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="([A-Z]{2}[0-9]{6}[A-Z]?)|([0-9]{2}[A-Z][0-9]{5})"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAMTRdateType">
    <xsd:restriction base="xsd:date">
      <xsd:maxInclusive value="2040-01-01"/>
      <xsd:minInclusive value="1851-01-01"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="MTR_SAaddressStructure">
    <xsd:annotation>
      <xsd:documentation>Included since the HMRC core InternationalAddressStructure does not provide the correct format.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="3" name="Line" type="MTR:MTR_SA28charStringType"/>
      <xsd:element minOccurs="0" name="ShortLine">
        <xsd:simpleType>
          <xsd:restriction base="MTR:MTR_SAstringType">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="18"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="PostCode" type="MTR:MTR_SApostCodeType"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="MTR_SAbuildingSocietyReferenceType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z0-9\-/]{1,18}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAcountryCodeType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z]{3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAfilenameType">
    <xsd:restriction base="MTR:MTR_SAstringType">
      <xsd:pattern value="[^£$#~|€/\\:*&quot;&lt;&gt;]+"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAmonetaryType">
    <xsd:restriction base="MTR:MTR_IRdecimalType">
      <xsd:maxExclusive value="10000000000.00"/>
      <xsd:minExclusive value="-10000000000.00"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAnonEmptyStringType">
    <xsd:restriction base="MTR:MTR_SAstringType">
      <xsd:pattern value=".*[^\s]+.*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAnonNegativeMonetaryType">
    <xsd:restriction base="MTR:MTR_IRdecimalType">
      <xsd:maxExclusive value="10000000000.00"/>
      <xsd:minInclusive value="0.00"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAnonNegativeNonZeroMonetaryType">
    <xsd:restriction base="MTR:MTR_IRdecimalType">
      <xsd:maxExclusive value="10000000000.00"/>
      <xsd:minExclusive value="0.00"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAnonZeroMonetaryType">
    <xsd:restriction base="MTR:MTR_SAmonetaryType">
      <xsd:pattern value="-?[0-9.]*[1-9]+[0-9.]*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SApostCodeType">
    <xsd:restriction base="MTR:MTR_SAstringType">
      <xsd:maxLength value="8"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAstringType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Za-z0-9 &amp;&apos;\(\)\*,\-\./@£]*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAtaxYearType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{4}-[0-9]{2}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_SAtelephoneNumberType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9 ]{1,20}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="MTR_TelephoneStructure">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="1" name="Number">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="35"/>
            <xsd:pattern value="[0-9\(\)\-\s]{1,35}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="Extension">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="6"/>
            <xsd:pattern value="[0-9]{1,6}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute use="optional" name="Type" type="MTR:MTR_WorkHomeType"/>
    <xsd:attribute use="optional" name="Mobile" type="MTR:MTR_YesNoType"/>
    <xsd:attribute use="optional" name="Preferred" type="MTR:MTR_YesNoType"/>
  </xsd:complexType>
  <xsd:simpleType name="MTR_UTRtype">
    <xsd:restriction base="xsd:string">
      <xsd:length value="10"/>
      <xsd:pattern value="[0-9]{10}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_WorkHomeType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="home"/>
      <xsd:enumeration value="work"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_YesNoType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="no"/>
      <xsd:enumeration value="yes"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MTR_YesType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="yes"/>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
