   function To_String( a : Egoalt_Rec; num_indents : Natural; loop_count : Natural := 0 ) return String is
      s : Unbounded_String;
   begin
      s := s & (num_indents*INDENT) & " ========== Egoalt";
      if( loop_count > 0 ) then
        s := s & "[ " & Format( loop_count ) & " ] ";
      end if;
      s := s & LINE_BREAK;
      return To_String( s );
   end To_String;
   





