-- auto ends
   
   type Gender_Type is ( male, female );
   
   subtype Benefit_Unit_Count is Natural range 0 .. Person_Count'Last; -- since bunumbers are pno of 1st person in hh
   subtype Benefit_Unit_Number is Benefit_Unit_Count range 1 .. Benefit_Unit_Count'Last;
   
   type Head_Or_Spouse is ( neither, head, spouse );
   subtype Head_Or_Spouse_Id is Head_Or_Spouse range head .. spouse;
   
   subtype Income_Count is Natural range 0 .. 40;
   subtype Income_Number is Income_Count range 1 .. Income_Count'Last;
   
   type Income_Rec_Array is array( Income_Number ) of Income_Rec;

   subtype Job_Count is Natural range 0 .. 60;
   subtype Job_Number is Job_Count range 1 .. Job_Count'Last;
   
   type Jobhstd_Array is array( Job_Number ) of Jobhstd_Rec;
   
   type Jobhist_Array is array( Job_Number ) of Jobhist_Rec;
   
   type Lifejob_Array is array( Job_Number ) of Lifejob_Rec;
   
   subtype Egoalt_Count is Natural range 0 .. 20;
   subtype Egoalt_Number is Egoalt_Count range 1 .. Egoalt_Count'Last;
   
   type Egoalt_Array is array( Egoalt_Number ) of Egoalt_Rec;

   type Child is record
      pid    : Sernum_Value  := MISSING_SERNUM;    -- pid : Cross-wave person identifier 
      pno    : Integer       := MISS;              -- person_number : Person number 
      age    : Age_Range;    
      wave   : Waves;
      sex    : HGSex_Type; -- Gender_Type;

      num_egoalts : Egoalt_Count := 0;
      egoalt : Egoalt_Array;
      
      indall : Indall_Rec;
      child_data : Child_Rec;   
      youth_data : Youth_Rec;
      childad    : Childad_Rec; -- adopted
      childnt    : Childnt_Rec;
      
   end record;

   type Adult is record
      pid          : Sernum_Value  := MISSING_SERNUM;    -- pid : Cross-wave person identifier 
      pno          : Integer       := MISS;              -- person_number : Person number 
      age          : Age_Range;      
      sex          : HGSex_Type; -- Gender_Type;
      wave         : Waves;
      
      num_egoalts  : Egoalt_Count := 0;
      egoalt       : Egoalt_Array;
      
      indall       : Indall_Rec;
      indresp      : Indresp_Rec;
      
      num_incomes  : Income_Count := 0;
      incomes      : Income_Rec_Array;
      
      num_jobhists : Job_Count := 0; 
      jobhist      : Jobhist_Array;
      
      cohabit      : Cohabit_Rec;
      indsamp      : Indsamp_Rec;
      lifemst      : Lifemst_Rec;
      
      num_lifejobs : Job_Count := 0;
      lifejob      : Lifejob_Array;
      
      num_jobhstds : Job_Count := 0;
      jobhstd      : Jobhstd_Array;

      child_data   : Child_Rec;   
      youth_data   : Youth_Rec;
      childad      : Childad_Rec; -- adopted
      childnt      : Childnt_Rec;
      
      xwavedat     : Xwavedat_Rec;
      xwaveten     : Xwlsten_Rec;
      
   end record;
   
   
   type BU_Allocation_Type is ( dss, all_16_plus );
   type BU_Count_Type is ( by_bu_number, by_sequence );
   

   type Benefit_Unit is tagged private;
   -- type Benefit_Unit is access Benefit_Unit;
   
   function Last_Head_Or_Spouse( bu : Benefit_Unit ) return Head_Or_Spouse_Id;
   function Num_With_Adult_Records( bu : Benefit_Unit ) return Adult_Count;
   function Num_Children( bu : Benefit_Unit ) return Child_Count;
   function Num_People( bu : Benefit_Unit ) return Person_Count;
   -- Get_Dependent_Adullt ... 
   function Get_Head_Or_Spouse( bu : Benefit_Unit; which : Head_Or_Spouse_Id ) return Adult; 
   function Get_With_Adult_Records( bu : Benefit_Unit; which : Adult_Number ) return Adult;
   function Get_Child( bu : Benefit_Unit; which : Child_Number ) return Child;
   function Get_Child( bu : Benefit_Unit; which : Child_Number ) return Adult;

   type Household is tagged private;
   type Household_Access is access Household;
   
   package Household_Vector_Package is new Ada.Containers.Vectors( Index_Type => Positive, Element_Type => Household_Access );
   subtype Household_Vector is Household_Vector_Package.Vector;

   function Get_Benefit_Unit( 
      hh : Household_Access;
      which      : Benefit_Unit_Number;
      definition : BU_Allocation_Type := dss; 
      count_type : BU_Count_Type      := by_sequence ) return Benefit_Unit;

   function Hhresp( hh : Household ) return Hhresp_Rec;
   function Hhsamp( hh : Household ) return Hhsamp_Rec;
   function Hid( hh : Household ) return Sernum_Value;
   procedure Free( hh : in out Household_Access );
   
   function Num_Benefit_Units( hh : Household ) return Benefit_Unit_Count; 
   function Num_People( hh : Household ) return Person_Count;
   function Get_All_Person_Numbers( hh : Household ) return Found_Person_Number_Array;
   -- note: returns by bhps pno, even if the internal sequence is different
   function Get_Person( hh : Household; pno : Person_Number ) return Adult;

   function Compare_Pno_Lt( left, right: Integer ) return Boolean;

   function Compare_Sernum_Lt( left, right: Sernum_Value ) return Boolean;
   
   function Compare_Adult_Record_Equal( left, right: Adult ) return Boolean;

   function Compare_Child_Record_Equal( left, right: Child ) return Boolean;

   package Complete_Adult_Map_Package is new Ada.Containers.Ordered_Maps( 
       Key_Type => Sernum_Value, 
       Element_Type => Adult, 
       "=" => Compare_Adult_Record_Equal,
       "<" => Compare_Sernum_Lt ); 
   subtype Complete_Adult_Map is Complete_Adult_Map_Package.Map;
   subtype Complete_Adult_Cursor is Complete_Adult_Map_Package.Cursor;
  
private
   
   type BU_Allocation is record
      is_adult    : Boolean := False;
      buno        : Benefit_Unit_Count := 0;
      bu_sequence : Benefit_Unit_Count := 0;
      chno        : Child_Count := 0;
      adno        : Head_Or_Spouse := neither;
      pno         : Person_Count := 0;
   end record;
   
   type BU_Allocation_Array is array( Person_Number ) of BU_Allocation;
   
   package Adult_Map_Package is new Ada.Containers.Ordered_Maps( 
       Key_Type => Integer, 
       Element_Type => Adult, 
       "=" => Compare_Adult_Record_Equal,
       "<" => Compare_Pno_Lt ); 
   subtype Adult_Map is Adult_Map_Package.Map;
   subtype Adult_Cursor is Adult_Map_Package.Cursor;

   package Child_Map_Package is new Ada.Containers.Ordered_Maps( 
       Key_Type => Integer, 
       Element_Type => Child, 
       "=" => Compare_Child_Record_Equal,
       "<" => Compare_Pno_Lt ); 
       
   subtype Child_Map is Child_Map_Package.Map;
   subtype Child_Cursor is Child_Map_Package.Cursor;
   
   type Found_BU_Number_Array  is array( Benefit_Unit_Number ) of Benefit_Unit_Count;
      
   type Benefit_Unit is tagged record
      last_adult_partner  : Head_Or_Spouse := head;
      num_people          : Person_Count := 0;
      num_adults          : Person_Count := 0;
      num_children        : Person_Count := 0;
      people_pos          : Found_Person_Number_Array := ( others => 0 );
      adult_pos           : Found_Person_Number_Array := ( others => 0 );
      child_pos           : Found_Person_Number_Array := ( others => 0 );
      
      bu_relationships    : BU_Allocation_Array;
      people              : Adult_Map;
   end record;
    
   type Household is tagged record
      hid               : Sernum_Value := MISS;
      people            : Adult_Map;
      hhresp            : Hhresp_Rec;
      hhsamp            : Hhsamp_Rec;
      num_benefit_units : Benefit_Unit_Count := 0;
      bu_relationships  : BU_Allocation_Array;
      people_pos          : Found_Person_Number_Array := ( others => 0 );
   end record;

   procedure Allocate_Household_Members( hh : in out Household; wave : Waves );
   procedure Free_Household is new Ada.Unchecked_Deallocation( Object => Household, Name => Household_Access );
   
end BHPS;
